[Unit]
Description=Launch the python script to receive the data from AIR ExDaMa ses


After=syslog.target
After=systemd-user-sessions.service


[Service]
Type=simple
ExecStart=/usr/local/bin/Stepmania_driver.sh
Restart=on-failure
RestartSec=10
KillMode=mixed

[Install]
WantedBy=multi-user.target